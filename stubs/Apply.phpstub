<?php

namespace FunctionalPHP\FantasyLand;

/**
 * @template T
 * @template-extends Functor<T>
 */
interface Apply extends Functor
{
    /**
     * @template U
     * @psalm-param Apply<U> $b
     * @psalm-return (T is callable ? Apply<U> : never-return)
     *
     * @param Apply $b
     *
     * @return Apply
     */
    public function ap(Apply $b) : Apply;
}
