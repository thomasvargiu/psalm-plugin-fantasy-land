<?php

namespace FunctionalPHP\FantasyLand;

/**
 * @template T
 */
interface Foldable
{
    /**
     * reduce :: (b -> a -> b) -> b -> b
     *
     * @template A
     * @param callable(A, T): A $function
     * @param A $accumulator
     * @return A
     */
    public function reduce(callable $function, $accumulator);
}
